<% provide :title, "Lv.5 - 全端挑戰" %>

<h1>帳號註冊</h1>
<br />
<%= form_for :overall do |f| %>
	<table class="table" data-toggle="table"><tbody><tr><td>
					<%= f.label :name, "暱稱" %></td><td>
  <%= f.text_field :name %></td></tr><tr><td>
  <%= f.label :email, "電子信箱" %></td><td>
  <%= f.text_field :email %></td></tr><tr><td>
  <%= f.label :birthday, "生日" %></td><td>
  <%= f.text_field :birthday, data:{ provide:'datepicker' } %></td></tr><tr><td>
  <%= f.label :password, "密碼" %></td><td>
  <%= f.password_field :password %></td></tr><tr><td>
  <%= f.label :password_confirmation, "再打一次密碼" %></td><td>
  <%= f.password_field :password_confirmation %></td></tr><tr><td>
  <%= f.submit "確定送出" %></td></tr></tbody></table>
  <br />
  <%= notice %>
<% end %>
