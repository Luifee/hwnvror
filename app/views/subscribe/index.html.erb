<% provide :title, "Email清單" %>

<h1>Email清單</h1>

<table class="table">
  <thead>
    <tr>
      <td>姓名/暱稱</td>
      <td>Email</td>
    </tr>
  </thead>
  <tbody>
    <% @subscribes.each do |subscribe| %>
    <tr>
      <td><%= subscribe.name %></td>
      <td><%= subscribe.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>
